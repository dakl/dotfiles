{
    "Prettier-SQL.SQLFlavourOverride": "bigquery",
    "Prettier-SQL.expressionWidth": 80,
    "Prettier-SQL.keywordCase": "upper",
    "Prettier-SQL.linesBetweenQueries": 0,
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true,
        "editor.formatOnType": true
    },
    "[terraform-vars]": {
        "editor.defaultFormatter": "hashicorp.terraform",
        "editor.formatOnSave": true,
        "editor.formatOnSaveMode": "file"
    },
    "[terraform]": {
        "editor.defaultFormatter": "hashicorp.terraform",
        "editor.formatOnSave": true,
        "editor.formatOnSaveMode": "file"
    },
    "[yaml]": {
        "editor.defaultFormatter": "ms-python.vscode-pylance"
    },
    "cSpell.customDictionaries": {
        "custom-words": {
            "name": "custom-words",
            "path": "~/.cspell/dictionary.txt",
            "addWords": true
        }
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.codeActionsOnSave": {},
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.fontSize": 16,
    "editor.formatOnSave": true,
    "editor.guides.bracketPairs": true,
    "editor.inlineSuggest.enabled": true,
    "editor.minimap.enabled": false,
    "editor.minimap.maxColumn": 80,
    "editor.minimap.showSlider": "always",
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "none",
    "editor.rulers": [
        100
    ],
    "editor.stickyScroll.enabled": true,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "files.exclude": {
        "**/*.pyc": true
    },
    "files.insertFinalNewline": true,
    "files.watcherExclude": {
        "**.mypy_cache": true,
        "**/.pytest_cache": true,
        "**/wandb": true
    },
    "git.autofetch": true,
    "git.confirmSync": false,
    "isort.check": true,
    "isort.showNotifications": "always",
    "jupyter.askForKernelRestart": false,
    "jupyter.interactiveWindow.textEditor.executeSelection": true,
    "jupyter.notebookFileRoot": "",
    "jupyter.runStartupCommands": [
        "'%load_ext autoreload\\n%autoreload 2'"
    ],
    "liveshare.featureSet": "stable",
    "python.analysis.autoFormatStrings": true,
    "python.analysis.extraPaths": [],
    "python.analysis.typeCheckingMode": "basic",
    "python.autoComplete.extraPaths": [],
    "python.diagnostics.sourceMapsEnabled": true,
    "python.languageServer": "Pylance",
    "python.testing.pytestArgs": [
        "-vv"
    ],
    "python.testing.pytestEnabled": true,
    "python.venvPath": "~/.local/share/virtualenvs/",
    "redhat.telemetry.enabled": false,
    "remote.SSH.remotePlatform": {
        "danielk": "linux",
        "musicgen-80gb": "linux",
        "192.168.68.117": "macOS"
    },
    "remote.SSH.remoteServerListenOnSocket": true,
    "security.workspace.trust.untrustedFiles": "open",
    "sql-formatter.uppercase": true,
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "terraform.languageServer.terraform.path": "/opt/homebrew/bin/terraform",
    "window.commandCenter": true,
    "workbench.colorTheme": "Default Light Modern",
    "workbench.editor.showIcons": true,
    "yaml.format.enable": false,
    "liveshare.accessibility.voiceEnabled": false,
    "terminal.integrated.fontSize": 14,
    "git.openRepositoryInParentFolders": "always",
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": true,
        "scminput": false,
        "csharp": false
    },
    "[svelte]": {
        "editor.defaultFormatter": "svelte.svelte-vscode"
    },
    "terminal.integrated.scrollback": 1000000,
    "terminal.integrated.persistentSessionScrollback": 10000,
    "github.copilot.editor.enableAutoCompletions": true,
    "gitlens.advanced.messages": {
        "suppressIntegrationRequestTimedOutWarning": true
    },
    "explorer.confirmDragAndDrop": false,
    "[csharp]": {
        "editor.defaultFormatter": "ms-dotnettools.csharp"
    },
    "editor.codeLens": false,
    "editor.inlineSuggest.suppressSuggestions": true,
    "cody.autocomplete.advanced.provider": "unstable-openai",
    "remote.SSH.defaultExtensions": [
        "anyscalecompute.anyscale-workspaces",
        "ms-python.python",
        "ms-toolsai.jupyter-renderers",
        "ms-toolsai.jupyter-keymap",
        "ms-toolsai.jupyter"
    ],
    "remote.SSH.configFile": "/Users/daniel.klevebring/.ssh/config",
    "yaml.schemas": {
        "/Users/daniel.klevebring/.vscode/extensions/continue.continue-1.0.7-darwin-arm64/config-yaml-schema.json": [
            ".continue/**/*.yaml"
        ]
    }
}
