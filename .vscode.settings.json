{
    "editor.renderWhitespace": "none",
    "editor.renderControlCharacters": true,
    "editor.rulers": [
        80,
        100,
        120
    ],
    "editor.minimap.showSlider": "always",
    "editor.fontSize": 16,
    "files.exclude": {
        "**/*.pyc": true
    },
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.testing.pytestArgs": ["-vv"],
    "python.venvPath": "~/.local/share/virtualenvs/",
    "python.formatting.provider": "black",
    "workbench.editor.showIcons": true,
    "workbench.statusBar.feedback.visible": false,
    "terminal.integrated.rendererType": "dom",
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.keymap": "chorded",
    "eslint.provideLintTask": true,
    "eslint.autoFixOnSave": true,
    "eslint.alwaysShowStatus": true,
    "editor.minimap.maxColumn": 80,
    "sort-imports.on-save": false,
    "workbench.iconTheme": "vscode-icons",
    "editor.minimap.enabled": false,
    "importMagic.indentWithTabs": false,
    "importMagic.maxColumns": 100,
    "git.confirmSync": false,
    "liveshare.featureSet": "stable",
    "python.linting.mypyEnabled": true,
    "importMagic.multiline": "parentheses",
    "workbench.colorTheme": "Default Light+",
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "jupyter.runStartupCommands": "'%load_ext autoreload\\n%autoreload 2'",
    "jupyter.notebookFileRoot": "",
    "jupyter.sendSelectionToInteractiveWindow": true,
    "jupyter.askForKernelRestart": false,
    "files.insertFinalNewline": true
}
